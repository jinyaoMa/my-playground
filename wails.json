{
  "name": "My Playground",
  "outputfilename": "my-playground",
  "assetdir": "frontend/panel/dist",
  "frontend:install": "npm install && npx lerna bootstrap",
  "frontend:build": "npx lerna --scope @my-playground/panel exec -- vite build --emptyOutDir",
  "frontend:dev:watcher": "npx lerna --scope @my-playground/panel exec -- vite",
  "frontend:dev:serverUrl": "auto",
  "wailsjsdir": "frontend/panel",
  "debounceMS": 1000,
  "devServer": "localhost:10000"
}
