{
  "name": "My Playground",
  "outputfilename": "my-playground",
  "assetdir": "backend/.assets",
  "frontend:install": "npm install && npx lerna bootstrap",
  "frontend:build": "npx lerna --scope @jinyaoma/my-playground-app exec -- vite build --emptyOutDir",
  "frontend:dev:watcher": "npx lerna --scope @jinyaoma/my-playground-app exec -- vite",
  "frontend:dev:serverUrl": "auto",
  "wailsjsdir": "frontend/app",
  "debounceMS": 1000,
  "devServer": "localhost:10000"
}
